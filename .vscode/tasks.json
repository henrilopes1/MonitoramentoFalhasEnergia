{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build: MyApp.UI",
            "type": "process",
            "command": "dotnet",
            "args": [
                "build",
                "${workspaceFolder}/MyApp.UI/MyApp.UI.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
